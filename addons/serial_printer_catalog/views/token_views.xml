<odoo>
    <record id="view_form_token" model="ir.ui.view">
        <field name="name">serial.printer.token.form</field>
        <field name="model">serial.printer.token</field>
        <field name="arch" type="xml">
            <form string="Token">
                <sheet>
                    <group>
                        <field name="token"/>
                        <field name="expiry_time"/>
                        <field name="username"/>
                        <field name="password"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_tree_token" model="ir.ui.view">
        <field name="name">serial.printer.token.list</field>
        <field name="model">serial.printer.token</field>
        <field name="arch" type="xml">
            <list string="Tokens">
                <field name="token"/>
                <field name="expiry_time"/>
                <field name="username"/>
            </list>
        </field>
    </record>

    <record id="action_token" model="ir.actions.act_window">
        <field name="name">Tokens</field>
        <field name="res_model">serial.printer.token</field>
        <field name="view_mode">list,form</field>
    </record>

    <menuitem id="menu_token"
              name="Tokens"
              parent="menu_root_catalog"
              sequence="80"
              action="action_token"/>
</odoo>
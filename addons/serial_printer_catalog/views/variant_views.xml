<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_serial_printer_variant_list" model="ir.ui.view">
        <field name="name">serial.printer.variant.list</field>
        <field name="model">serial.printer.variant</field>
        <field name="arch" type="xml">
            <list string="Variantes">
                <field name="name"/>
                <field name="toptex_id"/>
            </list>
        </field>
    </record>

    <record id="view_serial_printer_variant_form" model="ir.ui.view">
        <field name="name">serial.printer.variant.form</field>
        <field name="model">serial.printer.variant</field>
        <field name="arch" type="xml">
            <form string="Variante">
                <group>
                    <field name="name"/>
                    <field name="toptex_id"/>
                    <field name="product_template_id"/>
                    <field name="attribute_ids" widget="many2many_tags"/>
                </group>
            </form>
        </field>
    </record>
</odoo>
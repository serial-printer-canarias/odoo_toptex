<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_serial_printer_variant_list" model="ir.ui.view">
        <field name="name">serial.printer.variant.list</field>
        <field name="model">serial.printer.variant</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="code"/>
                <field name="attribute_id"/>
            </list>
        </field>
    </record>

    <record id="action_serial_printer_variant_list" model="ir.actions.act_window">
        <field name="name">Variantes</field>
        <field name="res_model">serial.printer.variant</field>
        <field name="view_mode">list</field>
    </record>
</odoo>
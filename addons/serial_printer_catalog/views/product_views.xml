<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_serial_printer_product_list" model="ir.ui.view">
        <field name="name">serial.printer.product.list</field>
        <field name="model">product.template</field>
        <field name="arch" type="xml">
            <list string="Productos API" create="false">
                <field name="name"/>
                <field name="default_code"/>
                <field name="type"/>
                <field name="list_price"/>
                <field name="standard_price"/>
            </list>
        </field>
    </record>

    <record id="action_serial_printer_product_list" model="ir.actions.act_window">
        <field name="name">Productos</field>
        <field name="res_model">product.template</field>
        <field name="view_mode">list,form</field>
        <field name="view_id" ref="view_serial_printer_product_list"/>
        <field name="context">{}</field>
    </record>
</odoo>
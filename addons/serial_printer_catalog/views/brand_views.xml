<odoo>
    <record id="view_brand_list" model="ir.ui.view">
        <field name="name">serial.printer.brand.list</field>
        <field name="model">serial.printer.brand</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
                <field name="code"/>
            </list>
        </field>
    </record>

    <record id="view_brand_form" model="ir.ui.view">
        <field name="name">serial.printer.brand.form</field>
        <field name="model">serial.printer.brand</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="code"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <menuitem id="serial_printer_catalog_root" name="CatÃ¡logo"/>
    <menuitem id="serial_printer_catalog_menu" name="Marcas" parent="serial_printer_catalog_root"/>

    <record id="action_serial_printer_brand" model="ir.actions.act_window">
        <field name="name">Marcas</field>
        <field name="res_model">serial.printer.brand</field>
        <field name="view_mode">list,form</field>
    </record>

    <menuitem id="menu_serial_printer_brand"
              name="Marcas"
              parent="serial_printer_catalog_menu"
              action="action_serial_printer_brand"/>
</odoo>
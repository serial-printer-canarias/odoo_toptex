<odoo>
    <record id="view_brand_list" model="ir.ui.view">
        <field name="name">serial.printer.brand.list</field>
        <field name="model">serial.printer.brand</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="code"/>
            </tree>
        </field>
    </record>

    <record id="view_brand_form" model="ir.ui.view">
        <field name="name">serial.printer.brand.form</field>
        <field name="model">serial.printer.brand</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="code"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <menuitem id="serial_printer_catalog_root" name="CatÃ¡logo"/>
    <menuitem id="serial_printer_catalog_menu" name="Marcas" parent="serial_printer_catalog_root"/>
    <act_window id="action_serial_printer_brand" name="Marcas"
        res_model="serial.printer.brand" view_mode="tree,form"
        menu_id="serial_printer_catalog_menu"/>
</odoo>